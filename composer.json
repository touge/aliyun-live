{
    "name": "touge/admin-aliyun-live",
    "description": "基于laravel-admin的阿里云直播控台台",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "touge", "aliyun live"],
    "homepage": "https://github.com/touge/admin-aliyun-live",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Yuan",
            "email": "lon.y@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "guzzlehttp/guzzle": "~6.0",
        "alibabacloud/client": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Touge\\AdminAliyunLive\\": "src/"
        }
